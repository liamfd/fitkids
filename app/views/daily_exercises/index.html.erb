<h1>Listing daily_exercises</h1>

<table>
  <tr>
    <th>Minutes exercised</th>
    <th>Day made</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @daily_exercises.each do |daily_exercise| %>
  <tr>
    <td><%= daily_exercise.minutes_exercised %></td>
    <td><%= daily_exercise.day_made %></td>
    <td><%= link_to 'Show', daily_exercise %></td>
    <td><%= link_to 'Edit', edit_daily_exercise_path(daily_exercise) %></td>
    <td><%= link_to 'Destroy', daily_exercise, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Daily exercise', new_daily_exercise_path %>
